<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.constellation">
    <!-- 必须加上网络请求权限，否则无法请求网络数据 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/statusBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".home.GuideActivity"></activity>
        <activity android:name=".home.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".luckfrag.LuckAnalysisActivity" />
        <activity android:name=".parnterfrag.ParnterAnalysisActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".starfrag.StarAnalysisActivity" />
    </application>

</manifest>